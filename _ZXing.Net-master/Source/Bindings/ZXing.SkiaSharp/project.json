{
	"name": "ZXing.SkiaSharp",
	"version": "0.16.3",
	"dependencies": {
		"SkiaSharp": "1.57.0"
	},
	"frameworks": {
		"net45": {
			"dependencies": {
				"zxing.net4.5": {
					"target": "project"
				}
			},
			"buildOptions": {
				"define": [ "NET45" ],
				"debugType": "full"
			}
		},
		"netstandard1.3": {
			"dependencies": {
				"NETStandard.Library": "1.6.0",
				"ZXing.Net": {
					"target": "project"
				}
			},
			"buildOptions": {
				"define": [ "NETSTANDARD" ],
				"debugType": "portable"
			}
		}
	},
	"buildOptions": {
		"optimize": true,
		"xmlDoc": true,
		"platform": "anycpu",
		"publicSign": false,
		"delaySign": false,
		"keyFile": "../../../Key/private.snk",
		"outputName": "zxing.skiasharp",
		"allowUnsafe": true,
		"compile": {
			"includeFiles": [
				"Properties/AssemblyInfo.cs",
				"BarcodeReader.cs",
				"BarcodeReader.Extensions.cs",
				"SKBitmapLuminanceSource.cs"
			]
		}
	},
	"configurations": {
		"Debug": {
			"buildOptions": {
				"xmlDoc": false,
				"optimize": false
			}
		}
	}
}
